<h1>Sign in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">

  <%= form_for(:sessions, url: sessions_path) do |f| %>
      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit 'Sign In'%>

  <% end %>
  </div>
</div>